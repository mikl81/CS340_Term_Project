<div class="pageDesc">
    <h1>Users</h1>
    <p>The users of Bits-N-Pixel</p>
</div>

{{! READ TABLE }}
<table>
    <thead>
        <tr>
            {{#each users.[0]}}
            <th>{{@key}}</th>
            {{/each}}
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
        <tr>
            <td>{{this.userID}}</td>
            <td>{{this.Username}}</td>
            <td>{{this.Email}}</td>
        {{/each}}
    </tbody>
</table>
